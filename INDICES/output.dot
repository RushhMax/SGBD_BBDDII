digraph BTree {
    rankdir = TB;
    ranksep = 3;
    nodesep = 0.3;

    /* 0 */
    node0 [shape=none, fontsize=18, margin=0, label=<
      <TABLE BORDER="0" COLOR="black" CELLBORDER="1" CELLSPACING="0" CELLPADDING="4">
        <TR>
          <TD PORT="0" BGCOLOR="lightgrey"></TD>
          <TD PORT="1" WIDTH="40" HEIGHT="35" FIXEDSIZE="TRUE"></TD>
